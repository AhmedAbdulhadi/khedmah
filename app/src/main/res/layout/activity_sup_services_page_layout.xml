<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_sup_services_page_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.bassam.khedmaapp.user.activitys.SupServicesPageActivity">

    <LinearLayout
        android:id="@+id/container_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include
            android:id="@+id/toolbar"
            layout="@layout/back_toolbar_layout" />
    </LinearLayout>

    <FrameLayout
        android:id="@+id/sup_services_container_body_activity"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1" >

    </FrameLayout>
</LinearLayout>
